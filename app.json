{
  "expo": {
    "name": "Mini Mart",
    "slug": "rn-starter",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "backgroundColor": "#f5f5f5",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#111111"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.wootcot.rn-starter"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#0b263d",
        "foregroundImage": "./assets/images/adaptive-icon.png"
      },
      "package": "com.wootcot.rnstarter"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-localization",
      "expo-asset",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}